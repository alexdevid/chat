  "require": {
    "ext-intl": ">=1.0.0",
    "symfony/symfony": "~2.4@stable",
    "symfony/monolog-bundle": "@stable",
    "symfony/web-profiler-bundle": "@stable",
    "symfony/assetic-bundle": "@stable",
    "sensio/generator-bundle": "@stable",
    "sensio/distribution-bundle": "~4.0",
